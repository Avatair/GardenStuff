{
    "multipart": [
        {
            "when": { "mount": "floor|pike" },
            "apply": { "model": "gardenstuff:candelabra/candle_center" }
        }, {
            "when": { "mount": "ceiling" },
            "apply": { "model": "gardenstuff:candelabra/hanger" }
        }, {
            "when": { "mount": "floor|pike|ceiling" },
            "apply": { "model": "gardenstuff:candelabra/candle_left_right" }
        }, {
            "when": { "mount": "floor|pike|ceiling" },
            "apply": { "model": "gardenstuff:candelabra/candle_left_right", "y": 90 }
        }, {
            "when": { "mount": "wall", "facing": "north" },
            "apply": { "model": "gardenstuff:candelabra/candle_wall_2", "y": 180 }
        }, {
            "when": { "mount": "wall", "facing": "north" },
            "apply": { "model": "gardenstuff:candelabra/candle_wall_3", "y": 180 }
        }, {
            "when": { "mount": "wall", "facing": "south" },
            "apply": { "model": "gardenstuff:candelabra/candle_wall_2" }
        }, {
            "when": { "mount": "wall", "facing": "south" },
            "apply": { "model": "gardenstuff:candelabra/candle_wall_3" }
        }, {
            "when": { "mount": "wall", "facing": "east" },
            "apply": { "model": "gardenstuff:candelabra/candle_wall_2", "y": 270 }
        }, {
            "when": { "mount": "wall", "facing": "east" },
            "apply": { "model": "gardenstuff:candelabra/candle_wall_3", "y": 270 }
        }, {
            "when": { "mount": "wall", "facing": "west" },
            "apply": { "model": "gardenstuff:candelabra/candle_wall_2", "y": 90 }
        }, {
            "when": { "mount": "wall", "facing": "west" },
            "apply": { "model": "gardenstuff:candelabra/candle_wall_3", "y": 90 }
        }
    ]
}